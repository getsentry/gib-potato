{
    "name": "getsentry/gib-potato",
    "description": "CakePHP skeleton app",
    "type": "project",
    "homepage": "https://sentry.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Sentry",
            "email": "accounts@sentry.io"
        }
    ],
    "require": {
        "php": ">=8.1",
        "cakephp/authentication": "^2.10.1",
        "cakephp/cakephp": "4.4.*",
        "cakephp/migrations": "^3.8.1",
        "cakephp/plugin-installer": "^1.3.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "lcobucci/jwt": "^4.3",
        "paragonie/csp-builder": "2.7",
        "sentry/sentry": "^3.19.1",
        "symfony/http-client": "^6.2.10"
    },
    "require-dev": {
        "cakephp/bake": "^2.9.3",
        "cakephp/cakephp-codesniffer": "^4.7",
        "phpstan/phpstan": "^1.10.15",
        "phpunit/phpunit": "^9.6.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-create-project-cmd": "App\\Console\\Installer::postInstall",
        "check": [
            "@test",
            "@cs-check",
            "@phpstan"
        ],
        "cs-check": "phpcs --colors -p  src/ tests/",
        "cs-fix": "phpcbf --colors -p src/ tests/",
        "phpstan": "phpstan analyse",
        "test": "phpunit --colors=always"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cakephp/plugin-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    }
}
